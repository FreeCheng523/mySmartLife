<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/dp_38" />
            <!--<solid android:color="@color/start_drive_color_highlight" />-->
            <solid android:color="@color/start_drive_color_highlight_press" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="@dimen/dp_38" />
            <solid android:color="@color/start_drive_color_highlight_unable" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/dp_38" />
            <solid android:color="@color/start_drive_color_highlight_normal" />
        </shape>
    </item>
</selector>